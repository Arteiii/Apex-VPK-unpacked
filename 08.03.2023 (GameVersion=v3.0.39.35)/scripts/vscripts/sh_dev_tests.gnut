void function test_tiptext( int tipNumber )
{
	string tip

	if( tipNumber < 10 )
	{
		tip = "#TIP_00" + tipNumber
	}
	else if ( tipNumber < 100 )
	{
		tip = "#TIP_0" + tipNumber
	}
	else
	{
		tip = "#TIP_" + tipNumber
	}

	string tip_localized = Localize( tip )

	printt( format( "%s(): Tip Text %s == %s ", FUNC_NAME(), tip, tip_localized ) )
	Obituary_Print_Localized( tip_localized )
}

