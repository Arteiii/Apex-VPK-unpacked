bool function Mastery_WeaponTrialsBecameUnlockedOrCompleted()
{
	entity player = GetLocalClientPlayer()
	if ( !IsValid( player ) )
		return false

	                                                                      
	foreach ( weapon, weaponConfig in fileLevel.weaponConfigMap )
	{
		foreach ( trialConfig in weaponConfig.trialList )
		{
			if ( DidWeaponTrialBecomeUnlocked( player, weaponConfig, trialConfig ) || DidWeaponTrialBecomeCompleted( player, weapon, trialConfig ) )
				return true
		}
	}

	return false
}

bool function DidWeaponTrialBecomeUnlocked( entity player, MasteryWeaponConfig weaponConfig, TrialConfig trialConfig )
{
	int currentXP         = player.GetPersistentVarAsInt( weaponConfig.persistenceKey_XP )
	int lastXP            = player.GetPersistentVarAsInt( weaponConfig.persistenceKey_SopmXP )
	bool trialWasUnlocked = false

	if ( currentXP > lastXP )
	{
		int lastLevel    = Mastery_CalculateLevelFromXP( lastXP )
		int currentLevel = Mastery_CalculateLevelFromXP( currentXP )
		trialWasUnlocked = !IsTrialUnlocked( lastLevel, trialConfig.tierLevel ) && IsTrialUnlocked( currentLevel, trialConfig.tierLevel )
	}

	return trialWasUnlocked
}

bool function DidWeaponTrialBecomeCompleted( entity player, ItemFlavor weapon, TrialConfig trialConfig )
{
	int currentStat        = player.GetPersistentVarAsInt( BuildPDefKey_TrialStat( weapon, trialConfig.tierLevel ) )
	int lastStat           = player.GetPersistentVarAsInt( BuildPDefKey_TrialStruct( weapon, trialConfig.tierLevel ) + PERSISTENCE_SOPM_STAT_SUFFIX )
	bool trialWasCompleted = currentStat > lastStat && lastStat < trialConfig.goal && currentStat >= trialConfig.goal

	return trialWasCompleted
}

array<MasteryWeapon_TrialStatusChangesPerWeapon> function Mastery_GetWeaponTrialStatusChanges()
{
	array<MasteryWeapon_TrialStatusChangesPerWeapon> result

	entity player = GetLocalClientPlayer()
	if ( !IsValid( player ) )
		return result

	foreach ( weapon, weaponConfig in fileLevel.weaponConfigMap )
	{
		MasteryWeapon_TrialStatusChangesPerWeapon newEntry
		newEntry.weapon = weapon

		array<MasteryWeapon_TrialQueryResult> ornull weaponTrialListState1 = Mastery_GetWeaponTrialListClonedTrialData( weapon )
		array<MasteryWeapon_TrialQueryResult> ornull weaponTrialListState2 = Mastery_GetWeaponTrialList( weapon )
		expect array<MasteryWeapon_TrialQueryResult>( weaponTrialListState1 )
		expect array<MasteryWeapon_TrialQueryResult>( weaponTrialListState2 )

		bool weaponHasTrialStatusChanges = false
		int newChallengesCompleted = 0

		foreach ( index, trialConfig in weaponConfig.trialList )
		{
			MasteryWeapon_TrialStatusChange statusChangeData
			statusChangeData.state1 = weaponTrialListState1[index]

			if ( DidWeaponTrialBecomeUnlocked( player, weaponConfig, trialConfig ) )
			{
				weaponHasTrialStatusChanges = true
				statusChangeData.statusChange = eWeaponTrialStatusChangeType.UNLOCKED
				statusChangeData.state1.trialData.isUnlocked = false
				statusChangeData.state2 = weaponTrialListState2[index]
			}
			else if ( DidWeaponTrialBecomeCompleted( player, weapon, trialConfig ) )
			{
				weaponHasTrialStatusChanges = true
				statusChangeData.statusChange = eWeaponTrialStatusChangeType.COMPLETED
				statusChangeData.state1.trialData.isCompleted = false
				statusChangeData.state1.trialData.currentValue = player.GetPersistentVarAsInt( BuildPDefKey_TrialStruct( weapon, trialConfig.tierLevel ) + PERSISTENCE_SOPM_STAT_SUFFIX )
				statusChangeData.state2 = weaponTrialListState2[index]
				newChallengesCompleted++
			}
			else
			{
				statusChangeData.statusChange = eWeaponTrialStatusChangeType.NONE
			}

			newEntry.trialStatusChangeList.append( statusChangeData )
		}

		MasteryWeapon_TrialStatusChange statusChangeData
		MasteryWeapon_TrialQueryResult finalTrial = expect MasteryWeapon_TrialQueryResult( Mastery_GetWeaponFinalTrialData( weapon ) )
		if ( newChallengesCompleted > 0 && finalTrial.trialData.isCompleted )
		{
			finalTrial.trialData.isCompleted = false
			finalTrial.trialData.currentValue -= newChallengesCompleted
			statusChangeData.statusChange = eWeaponTrialStatusChangeType.COMPLETED
			statusChangeData.state1 = finalTrial
			statusChangeData.state2 = expect MasteryWeapon_TrialQueryResult( Mastery_GetWeaponFinalTrialData( weapon ) )
			newEntry.trialStatusChangeList.append( statusChangeData )
		}

		if ( weaponHasTrialStatusChanges )
			result.append( newEntry )
	}

	return result
}
