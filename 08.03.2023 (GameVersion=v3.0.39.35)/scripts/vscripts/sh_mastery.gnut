array<MasteryReward> function Mastery_GetMatchRewards()
{
	array<MasteryReward> rewards
	int completeWeaponCount = 0
	bool hasNewCompletedTrial = false
	foreach ( weapon, weaponConfig in fileLevel.weaponConfigMap )
	{
		int completeTrialCount         = 0
		int completedTrialCount_Five   = 0
		bool hasNewCompletedTrial_Five = false
		int weaponLevel                = GetPersistentVarAsInt( weaponConfig.persistenceKey_Level )
		bool containSpecialBadge       = false
		                                          
		foreach ( TrialConfig trialConfig in weaponConfig.trialList )
		{
			int currentStat        = GetPersistentVarAsInt( BuildPDefKey_TrialStat( weapon, trialConfig.tierLevel ) )
			int sopmStat           = GetPersistentVarAsInt( BuildPDefKey_TrialStruct( weapon, trialConfig.tierLevel ) + PERSISTENCE_SOPM_STAT_SUFFIX )
			bool isRewardFiveTrial = trialConfig.tierLevel <= FINAL_REWARD_FIVE_INDEX
			if ( currentStat >= trialConfig.goal )
			{
				completeTrialCount += 1
				if ( isRewardFiveTrial )
					completedTrialCount_Five += 1
				if ( sopmStat < trialConfig.goal )
				{
					hasNewCompletedTrial = true
					if ( isRewardFiveTrial )
						hasNewCompletedTrial_Five = true
					                
					foreach ( reward in trialConfig.rewards )
					{
						if ( trialConfig.specialBadge == reward.flav )
						{
							if ( containSpecialBadge )
								continue
							containSpecialBadge = true
							Assert( IsItemFlavorStructValid( ( expect ItemFlavor( trialConfig.specialBadge ) ).guid, eValidation.ASSERT ) )
							Assert( ItemFlavor_GetGRXMode( expect ItemFlavor( trialConfig.specialBadge ) ) == eItemFlavorGRXMode.REGULAR )
							MasteryReward masteryReward
							masteryReward.flav = expect ItemFlavor( trialConfig.specialBadge )
							masteryReward.quantity = 1
							masteryReward.level = GRX_GetItemTier( ItemFlavor_GetGRXIndex( reward.flav ) ) - 2                                                                                                        
							rewards.append( masteryReward )
						}
						else
						{
							rewards.append( reward )
						}
					}
				}
			}
		}
		                              
		if ( ( completedTrialCount_Five >= FINAL_REWARD_FIVE_INDEX + 1 ) && hasNewCompletedTrial_Five )
		{
			foreach ( reward in weaponConfig.finalRewardFive )
			{
				rewards.append( reward )
			}
		}
		                         
		if ( completeTrialCount == weaponConfig.trialList.len() )
		{
			completeWeaponCount += 1
		}
	}
	                    
	if ( completeWeaponCount == fileLevel.weaponConfigMap.len() && hasNewCompletedTrial )
	{
		foreach ( reward in fileLevel.allWeaponRewards )
		{
			rewards.append( reward )
		}
	}
	return rewards
}

