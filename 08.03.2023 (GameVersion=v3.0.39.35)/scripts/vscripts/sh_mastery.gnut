				statusChange = "unlocked"
			else if ( DidWeaponTrialBecomeCompleted( player, weapon, trialConfig ) )
				statusChange = "completed"

			printt( "    trial", index + ":", statusChange )
		}
	}
}
#endif

bool function Mastery_IsEnabled()
{
	return GetCurrentPlaylistVarInt( "enable_mastery_weapon", 1 ) == 1
}